      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ignitiondating.datingapp",
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "cafe5c42-7da0-4c09-b049-cc9cc035122e"
      }
    },
    "owner": "tyhenkel"

  }
}
{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx":"react-native",
    "strict": true,
   "baseUrl": "./src",
    "paths": {
      "@svgs/*": ["assets/svgs/*"],
      "@fonts/*": ["assets/fonts/*"],
      "@images/*": ["assets/images/*"],
      "@screens": ["screens"],
      "@app-types": ["types"],
      "@app-types/*": ["types/*"],
      "@app-data": ["data"],
      "@app-data/*": ["data/*"],
      "@constants": ["constants"],
      "@constants/*": ["constants/*"],
    }
  }
}